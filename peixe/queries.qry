
query TodolosPeixes {
  description: "Devolve todos os peixes do rexistro"
  statement:  
    SELECT org.peixeencadeado.peixe.Peixe
}

query PeixePorPesqueira {
  description: "Devolve todos os peixes capturados por unha organizaci√≥n"
  statement:  
    SELECT  org.peixeencadeado.peixe.Peixe
  	WHERE   (pesqueira.orgId == _$orgId )
}


